{
  "name": "simple-blog",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^4.1.2",
    "halogen": "^0.2.0",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-lines-ellipsis": "^0.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-s-alert": "^1.3.0",
    "react-spinkit": "^3.0.0",
    "redux": "^3.7.0",
    "redux-form": "^6.8.0",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-sass": "^3.1.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "react-scripts": "1.0.7",
    "run-sequence": "^1.2.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
